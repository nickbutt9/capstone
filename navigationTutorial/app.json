{
  "expo": {
    "name": "navigationTutorial",
    "owner": "bme23-capstone",
    "slug": "navigationTutorial",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": ["@config-plugins/react-native-ble-plx"],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.paulchang04.navigationTutorial"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.paulchang04.navigationTutorial",
      "permission":["android.permission.BLUETOOTH_SCAN", "android.permission.ACCESS_FINE_LOCATION"]
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "d3f6bd65-f785-42d2-9d9e-8599f7f27808"
      }
    }
  }
}
